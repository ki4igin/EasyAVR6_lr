        "gccArgs": [
            "-ffunction-sections",
            "-fdata-sections",
            "-fpack-struct",
            "-fshort-enums",
            "-mshort-calls",
            "-std=gnu11",
            "-Winline",
            "-Wall",
            "-Wextra",
            "-MMD",
            "-MP"
        ]
